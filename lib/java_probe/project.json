{
	"name": "java_probe",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"projectType": "library",
	"sourceRoot": "lib/java_probe",
	"targets": {
		"build": {
			"executor": "nx:run-commands",
			"options": {
				"cwd": "lib/java_probe",
				"commands": [
					"javac --release 8 -d build JavaProbe.java",
					"jar cfm build/JavaProbe.jar manifest.txt -C build ."
				],
				"parallel": false
			}
		}
	},
	"tags": []
}
